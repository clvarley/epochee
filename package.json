{
  "name": "epochee",
  "version": "0.1.0",
  "description": "Sane datetime manipulation.",
  "homepage": "https://github.com/clvarley/epochee#readme",
  "keywords": [
    "date",
    "time",
    "datetime",
    "calculation",
    "manipulation",
    "add",
    "subtract"
  ],
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "pretest": "tsc -p tsconfig.spec.json",
    "test": "mocha --recursive",
    "posttest": "rm $(find ./src ./test -name \\*.js)",
    "tidy": "rm -R ./types && rm -R ./dist",
    "build": "npm run tidy && tsc"
  },
  "author": "clvarley",
  "license": "MIT",
  "peerDependencies": {
    "date-write": ">= 1.0.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "typescript": "^4.9.4"
  }
}
